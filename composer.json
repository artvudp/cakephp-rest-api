{
    "name": "vudp/cakephp-rest-api",
    "description": "CakePHP 4 plugin to provide basic support for building REST API services",
    "keywords": ["cakephp", "rest", "api"],
    "type": "cakephp-plugin",
    "homepage": "https://github.com/cakephp/debug_kit",
    "license": "MIT",
	"minimum-stability": "dev",
    "authors": [
        {
            "name": "Creditoh",
            "homepage": "http://www.creditoh.com",
            "role": "Author"
        },
        {
            "name": "Community",
            "homepage": "https://github.com/multidots/cakephp-rest-api/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/jacreditoh/cakephp-rest-api/issues",
        "source": "https://github.com/jacreditoh/cakephp-rest-api"
    },
    "require": {
        "php": ">=7.2.0",
        "cakephp/cakephp": "4.*",
        "firebase/php-jwt": "^4.0"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "^4.0",
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "RestApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RestApi\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests",
            "TestApp\\": "tests/test_app/TestApp"
        }
    }
}
